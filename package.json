{
  "name": "depbot",
  "version": "0.1.0",
  "private": true,
  "description": "Check freshness of your deno dependencies",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map",
    "test": "jest",
    "all": "yarn run build && yarn run format && yarn run lint && yarn run package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@typescript-eslint/parser": "^4.0.1",
    "@typescript-eslint/typescript-estree": "^4.0.1",
    "axios": "^0.20.0",
    "glob": "^7.1.6",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.2",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.12",
    "@types/node": "^14.0.23",
    "@types/semver": "^7.3.3",
    "@vercel/ncc": "^0.24.0",
    "eslint": "^7.8.0",
    "eslint-plugin-github": "^4.0.1",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "jest-circus": "^26.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
